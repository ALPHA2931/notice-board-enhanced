# Example Environment Variables for Production
# Copy your actual values here

# Database (from Supabase)
DATABASE_URL="postgresql://postgres.xxx:[YOUR-PASSWORD]@aws-0-us-west-1.pooler.supabase.com:6543/postgres"

# JWT Configuration
JWT_SECRET="your-super-secret-jwt-key-change-in-production-make-it-at-least-32-characters-long"
JWT_EXPIRES_IN="7d"

# Server Configuration
NODE_ENV="production"
PORT=3001

# Frontend URL (will be provided by Vercel)
FRONTEND_URL="https://your-project-name.vercel.app"
CORS_ORIGIN="https://your-project-name.vercel.app"
